    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.4",
    "@vueuse/core": "^9.13.0",
    "unocss": "^0.50.4",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-open-ide": "workspace:*",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@iconify-json/carbon": "^1.1.16",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "@vue/test-utils": "^2.3.1",
    "bumpp": "^9.0.0",
    "eslint": "^8.35.0",
    "happy-dom": "^8.9.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import-api": "^0.0.2",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-prop": "^0.0.7",
    "unplugin-vue-sfc-name": "^0.0.5",
    "vite": "^4.1.4",
